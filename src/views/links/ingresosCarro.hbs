<div class="container">
    <div class="col-md-4 mx-auto p-4">
        <h1 class="text-center"> {{link3.title}} </h1> 
        <h2 class="text-center"> {{link3.description}} </h2>
        <a href="{{ link3.url }}" class="btn btn-warning btn btn-block" target="_blank" >Ubicación</a>
    </div>

    <table class="table table-striped">
        
            <tr class="table-warning">
                <th style="width:30%"><h5>Ganancia del carro</h2> </td>
                <th style="width:30%"></td>
                <th style="width:30%"></td>
                <th style="width:30%">${{totales.ganacia}}</td>
            </tr>
        
    </table>

    <!--      ____________________Ingresos_________________             -->

    <table class="table table-striped mx-auto">
        <div class="row ">
            <div class="col-4">
                <h2>Ingresos</h2>
            </div>
        </div>
        
        {{#if links}}
        <thead class="thead-dark">
            <tr>
                <th style="width:30%"> Rentado a </th>
                <th style="width:30%"> Fecha </th>
                <th style="width:30%"> Valor </th>
                <th style="width:30%"> Historial </th>
            </tr>
        </thead>
        {{/if}}
        {{#each links}}
        <tbody>
            <tr>
                <td>{{nombre}}</td>
                <td>{{timeago3 fechaInicio}}</td>
                <td>${{precio}}</td>
                <td>
                    <a href="/links/detalles/{{id_historial}}" class="btn btn-info" > detalles </a>
                </td>
            </tr>
        </tbody>
    
    {{else}}
    <div class="col-md-4 mx-auto">
        <div class="card card-body text-center">
            <p>No hay Ingresos para este carro</p>
        </div>
    </div>
    {{/each}}
    {{#if links}}
        <thead>
            <tr class="table-success">
                <td><h5>Total</h5> </td>
                <td></td>
                <td> <h6> ${{totales.ingreso}} </h6></td>
                <td>
                    
                </td>
            </tr>
        </thead>
        </table>
    {{/if}}
    <br><br>
    <!--      ____________________Gastos_________________             -->
    
    <table class="table table-striped">
        <div class="row">
            <div class="col-4 ">
                <h2>Gastos</h2>
            </div>
            <div class="col-3 ml-auto form-group">
                <a href="/links/gastos/{{link3.id}}" class="btn btn-success btn-block" > Agregar Gastos </a>
            </div>
        </div>
            {{#if links2}}
            <thead class="thead-dark">
                <tr>
                    <th style="width:30%"> Gasto </th>
                    <th style="width:30%"> Fecha </th>
                    <th style="width:30%"> Valor </th>
                    <th style="width:30%"> Editar </th>
                </tr>
            </thead>
            {{/if}}
            {{#each links2}}
            <tbody>
                <tr>
                    <td>{{ingreso}}</td>
                    <td>{{timeago3 fecha}}</td>
                    <td>${{valor}}</td>
                    <td>
                        <a href="/links/editGastos/{{id_ingreso}}" class="btn btn-info" > editar </a>
                    </td>
                </tr>
            </tbody>
    
        {{else}}
            <div class="col-md-5 mx-auto p-4">
                <div class="card card-body text-center">
                    <p>No hay Gastos para este carro</p>
                </div>
            </div>
        
        {{/each}}
        {{#if links2}}
        <thead>
                <tr class="table-danger">
                    <td><h5>Total</h5> </td>
                    <td></td>
                    <td> <h6> ${{totales.gasto}} </h6></td>
                    <td>
                       
                    </td>
                </tr>
        </thead>
        </table>
    {{/if}}
    </table>
    <br><br>
    <!--      ____________________Mantenimientos_________________             -->
    
    <table class="table table-striped">
        <div class="row">
            <div class="col-4">
                <h2>Mantenimientos</h2>
            </div>
            <div class="col-3 ml-auto form-group">
                <a href="/links/mantenimiento/{{link3.id}}" class="btn btn-success btn-block" > Agregar Mantenimiento </a>
            </div>
        </div>
        {{#if links4}}
        <thead class="thead-dark">  
            <tr>
                <th style="width:30%"> Mantenimiento </th>
                <th style="width:30%"> Km </th>
                <th style="width:30%"> Fecha  </th>
                <th style="width:30%"> Acción </th>
            </tr>
        </thead>
        {{/if}}
        {{#each links4}}
        <tbody>
            <tr>
                <td>{{nombre}}</td>
                <td>{{km}}</td>
                <td>{{timeago3 fecha}}</td>
                <td>
                    <a href="/links/gastoRealizado/{{id_mantenimiento}}" class="btn btn-info">Ralizado</a>
                </td>
            </tr>
        </tbody>
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>No hay Mantenimientos para este carro</p>
            </div>
        </div>
        {{/each}}
    </table>
    <br><br>

    <!--      ____________________Porcentajes_________________             -->
    
    <table class="table table-striped">
        <div class="row">
            <div class="col-6">
                <h2>Porcentajes de ganancias</h2>
            </div>
            <div class="col-3 ml-auto form-group">
                <a href="/links/porcentaje/{{link3.id}}" class="btn btn-success btn-block" > Agregar porcentaje </a>
            </div>
        </div>
        {{#if links5}}
        <thead class="thead-dark">  
            <tr>
                <th style="width:30%"> Porcentaje </th>
                <th style="width:30%"> Valor  </th>
                <th style="width:30%"> Total </th>
                <th style="width:30%"> Acción </th>
            </tr>
        </thead>
        {{/if}}
        {{#each links5}}
        <tbody>
            <tr>
                <td>{{descripcion}}</td>
                <td>{{valor}}%</td>
                <td>${{total}}</td>
                <td>
                    <a href="/links/editPorcentaje/{{id_porcentaje}}" class="btn btn-info">editar</a>
                </td>
            </tr>
        </tbody>
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>No hay Porcentajes para este carro</p>
            </div>
        </div>
        {{/each}}
    </table>
</div>

